<svg width="512" height="512" viewBox="0 0 512 512" xmlns="http://www.w3.org/2000/svg">
  <!-- Fundo com gradiente -->
  <defs>
    <linearGradient id="backgroundGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#3b82f6;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1d4ed8;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="iconGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#ffffff;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#e5e7eb;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Fundo circular -->
  <circle cx="256" cy="256" r="240" fill="url(#backgroundGradient)" stroke="#1e40af" stroke-width="8"/>
  
  <!-- Ícone de calculadora/gráfico -->
  <g fill="url(#iconGradient)">
    <!-- Livro/documento base -->
    <rect x="120" y="140" width="272" height="232" rx="20" ry="20" fill="white" stroke="#3b82f6" stroke-width="4"/>
    
    <!-- Linhas do documento -->
    <line x1="160" y1="180" x2="352" y2="180" stroke="#3b82f6" stroke-width="3"/>
    <line x1="160" y1="210" x2="320" y2="210" stroke="#3b82f6" stroke-width="3"/>
    <line x1="160" y1="240" x2="352" y2="240" stroke="#3b82f6" stroke-width="3"/>
    
    <!-- Gráfico/barras representando notas -->
    <rect x="160" y="280" width="30" height="60" fill="#10b981" rx="4"/>
    <rect x="210" y="260" width="30" height="80" fill="#f59e0b" rx="4"/>
    <rect x="260" y="290" width="30" height="50" fill="#ef4444" rx="4"/>
    <rect x="310" y="250" width="30" height="90" fill="#10b981" rx="4"/>
    
    <!-- Símbolo de graduação no canto -->
    <circle cx="340" cy="170" r="15" fill="#1d4ed8"/>
    <path d="M330 170 L340 160 L350 170 L340 175 Z" fill="white"/>
  </g>
  
  <!-- Texto CR -->
  <text x="256" y="120" font-family="Arial, sans-serif" font-size="48" font-weight="bold" text-anchor="middle" fill="white">CR</text>
</svg>
